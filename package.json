{
  "name": "liberdus",
  "version": "1.0.0",
  "description": "Liberdus is a payment network that allows users to govern their own money system in a democratic fashion",
  "main": "dist/index.js",
  "scripts": {
    "pm2": "cross-env PM2_HOME='./.pm2' pm2 --node-args='--experimental-modules'",
    "start": "node scripts/start.js && node dist/index.js",
    "stop": "node scripts/stop.js",
    "clean": "node scripts/clean.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-template": "node scripts/update-template.js",
    "update-docker-dev": "docker build -t registry.gitlab.com/liberdus/server:dev -f ./dev.Dockerfile . && docker push registry.gitlab.com/liberdus/server:dev",
    "update-docker": "docker build -t registry.gitlab.com/liberdus/server:latest -f ./Dockerfile . && docker push registry.gitlab.com/liberdus/server:latest",
    "check": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "author": "Kyle Shifflett",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "chalk-pipe": "^3.0.0",
    "decimal.js": "^10.2.0",
    "deepmerge": "^4.2.2",
    "dot-prop": "^5.1.1",
    "esm": "^3.2.25",
    "fast-stable-stringify": "^1.0.0",
    "heapdump": "^0.3.15",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nodemailer": "^6.3.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "shardus-crypto-utils": "git+https://gitlab.com/shardus/shardus-crypto-utils.git",
    "shardus-global-server-dist": "git+https://gitlab.com/shardus/global/shardus-global-server-dist.git",
    "sqlite3": "^4.1.1",
    "streamroller": "^2.2.3",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@types/decimal.js": "^7.4.0",
    "@types/express": "^4.17.2",
    "@types/heapdump": "^0.3.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "archive-server": "git+https://gitlab.com/shardus/archive/archive-server.git",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "execa": "^1.0.0",
    "gts": "^2.0.0",
    "hygen-create": "^0.2.1",
    "monitor-server": "git+https://gitlab.com/shardus/monitor-server.git",
    "pm2": "^4.1.2",
    "prettier": "^1.19.1",
    "shelljs": "^0.8.3",
    "standard": "^12.0.1",
    "typescript": "~3.7.0",
    "yarpm": "^0.2.1"
  }
}
